    "spec": {
        "ANP": {
            "CNPJ": {
                "$ncnpj": "cnpj"
            },
            "Cód. Agente": "anp.cod_agente",
            "Data Fim da Situação": "anp.data_fim_situacao",
            "Data Inicio da Situação": "anp.data_inicio_situacao",
            "Data Publicação": "anp.data_publicacao",
            "Número de Autorização": "anp.numero_autorizacao",
            "Razão Social": "razao_social",
            "Situação": "anp.situacao",
            "Tipo de Ato": "anp.tipo_ato"
        },
        "CNAEs Secundários": {
            "$for": {
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Classe": "classe",
                    "Desc. Classe": "desc_classe",
                    "Desc. Divisão": "desc_divisao",
                    "Desc. Grupo": "desc_grupo",
                    "Desc. Seção": "desc_secao",
                    "Desc. Subclasse": "desc_subclasse",
                    "Divisão": "divisao",
                    "Grupo": "grupo",
                    "Ramo de Atividade": "ramo_de_atividade",
                    "Razão Social": "super$.razao_social",
                    "Segmento": "segmento",
                    "Seção": "secao",
                    "Subclasse": "subclasse"
                },
                "$prop": "atividades_secundarias"
            }
        },
        "CNES": {
            "Atividade": "cnes.atividade",
            "CNPJ": {
                "$ncnpj": "cnpj"
            },
            "Estabelecimento": "cnes.tipo_estabelecimento",
            "Qtd. Leitos SUS": "cnes.qtde_leitos_sus",
            "Qtd. Total de Leitos": "cnes.qtde_leitos_sus",
            "Qtd. Total de Leitos Não SUS": "cnes.qtde_leitos_sus",
            "Razão Social": "razao_social",
            "Tipo de Estabelecimento": "cnes.estabelecimento"
        },
        "COMEX": {
            "$for": {
                "$format": {
                    "Ano": "ano",
                    "Atividade": "tipo_atividade",
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Razão Social": "super$.razao_social"
                },
                "$prop": "comex"
            }
        },
        "Educação": {
            "CNPJ": {
                "$ncnpj": "cnpj"
            },
            "Docentes": "educacao.qtde_docentes",
            "Docentes Básico": "educacao.qtde_docentes_basico",
            "Docentes EJA": "educacao.qtde_docentes_especial",
            "Docentes Especial": "educacao.qtde_docentes_especial",
            "Docentes Fundamental": "educacao.qtde_docentes_fundamental",
            "Docentes Infantil": "educacao.qtde_docentes_infantil",
            "Docentes Médio": "educacao.qtde_docentes_medio",
            "Docentes Profissional": "educacao.qtde_docentes_profissional",
            "Matrículas": "educacao.qtde_matriculas",
            "Matrículas Básico": "educacao.qtde_matriculas__basico",
            "Matrículas EJA": "educacao.qtde_matriculas_especial",
            "Matrículas Especial": "educacao.qtde_matriculas_especial",
            "Matrículas Fundamental": "educacao.qtde_matriculas_fundamental",
            "Matrículas Infantil": "educacao.qtde_matriculas_infantil",
            "Matrículas Médio": "educacao.qtde_matriculas_medio",
            "Matrículas Profissional": "educacao.qtde_matriculas_profissional",
            "Razão Social": "razao_social"
        },
        "Emails de Empresas": {
            "$for": {
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Email": "email",
                    "Pertence ao Contador": {
                        "$switch": {
                            "$cases": [
                                {
                                    "$case": {
                                        "pertence_contador": true
                                    },
                                    "$use": {
                                        "$literal": "Sim"
                                    }
                                },
                                {
                                    "$case": {
                                        "pertence_contador": false
                                    },
                                    "$use": {
                                        "$literal": "Não"
                                    }
                                }
                            ]
                        }
                    },
                    "Razão Social": "super$.razao_social"
                },
                "$prop": "emails"
            }
        },
        "Contatos de Pessoas": {
            "$for": {
                "$filter": {
                    "cargo": {
                        "$exists": true
                    }
                },
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Cargo": { "$upper": "cargo" },
                    "Email": { "$lower": "email" },
                    "Linkedin": "linkedin",
                    "Nome": "nome",
                    "Nome Fantasia": "super$.nome_fantasia",
                    "Razão Social": "super$.razao_social",
                    "Status": "validacao"
                },
                "$prop": "profiles"
            }
        },
        "Empresa": {
            "Bairro": "bairro",
            "Capital Social": { "$string": "capital_social" },
            "CEP": {
                "$string": "cep"
            },
            "CNAE Principal Desc. Divisão": "cnae_principal_desc_divisao",
            "CNAE Principal Desc. Subclasse": "cnae_principal_desc_subclasse",
            "CNAE Principal Desc. Grupo": "cnae_principal_desc_grupo",
            "CNAE Principal Desc. Seção": "cnae_principal_desc_secao",
            "CNAE Principal Desc. Classe": "cnae_principal_desc_classe",
            "CNAE Principal Subclasse": {
                "$string": "cnae_principal_subclasse"
            },
            "CNPJ": {
                "$ncnpj": "cnpj"
            },
            "Data de Abertura": {
                "$date": "data_inicio_atividade"
            },
            "Endereço": "endereco",
            "Faixa de Faturamento Grupo": "faixa_faturamento_grupo",
            "Faixa de Funcionários Grupo": "faixa_funcionarios_grupo",
            "Forma de Tributação": "forma_tributacao",
            "Frota de Caminhão Grupo": "qtde_caminhao_grupo",
            "Frota de Ônibus Grupo": "qtde_onibus_grupo",
            "Latitude": "lat",
            "Logradouro": "logradouro",
            "Longitude": "lon",
            "Matriz": {
                "$switch": {
                    "$cases": [
                        {
                            "$case": {
                                "matriz": true
                            },
                            "$use": {
                                "$literal": "Sim"
                            }
                        },
                        {
                            "$case": {
                                "matriz": false
                            },
                            "$use": {
                                "$literal": "Não"
                            }
                        }
                    ]
                }
            },
            "Município": "municipio",
            "Natureza Jurídica": "natureza_juridica",
            "Nome Fantasia": "nome_fantasia",
            "Público ou Privada": "empresa_publico_privada",
            "Quantidade de Filiais": "qtde_filiais",
            "Raiz CNPJ": {
                "$string": "raiz_cnpj"
            },
            "Razão Social": "razao_social",
            "Setor": "segmento",
            "Situação Cadastral": "situacao_cadastral",
            "Subsetor": "ramo_de_atividade",
            "Total em Dívidas": "divida.total",
            "UF": "uf"
        },
        "Forma de Tributação": {
            "$for": {
                "$format": {
                    "Ano": "ano",
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Forma de Tributação": "forma_tributacao",
                    "Razão Social": "super$.razao_social"
                },
                "$prop": "forma_tributacao"
            }
        },
        "Serviços Entrega de Comida": {
            "CNPJ": {
                "$ncnpj": "cnpj"
            },
            "Categoria": "servico_entrega_comida.categoria",
            "Preço": "servico_entrega_comida.preco",
            "Razão Social": "razao_social"
        },
        "Sites": {
            "$for": {
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Pertence ao Contador": {
                        "$switch": {
                            "$cases": [
                                {
                                    "$case": {
                                        "pertence_contador": true
                                    },
                                    "$use": {
                                        "$literal": "Sim"
                                    }
                                },
                                {
                                    "$case": {
                                        "pertence_contador": false
                                    },
                                    "$use": {
                                        "$literal": "Não"
                                    }
                                }
                            ]
                        }
                    },
                    "Razão Social": "super$.razao_social",
                    "Tecnologias": {
                        "$joinby": {
                            "$prop": "tecnologias",
                            "$separator": ", "
                        }
                    },
                    "URL": "site"
                },
                "$prop": "sites"
            }
        },
        "Sócios": {
            "$for": {
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "CNPJ/CPF Sócio": "cnpj_cpf_socio",
                    "Código Faixa Etária": "faixa_etaria_socio",
                    "Código País": "pais",
                    "Código de Qualificação": "qualificacao_socio",
                    "Data de Entrada na Sociedade": {
                        "$date": "data_entrada_sociedade"
                    },
                    "Faixa Etária": "faixa_etaria_socio",
                    "Identificador": "identificador_socio",
                    "Nome": "nome_socio",
                    "País": "pais",
                    "Qualificação": "qualificacao_socio",
                    "Qualificação Representante Legal": "qualificacao_representante_legal",
                    "Razão Social": "super$.razao_social"
                },
                "$prop": "socios"
            }
        },
        "Telefones": {
            "$for": {
                "$format": {
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Fixo Ou Móvel": "fixo_movel",
                    "Pertence a Contador": {
                        "$switch": {
                            "$cases": [
                                {
                                    "$case": {
                                        "pertence_contador": true
                                    },
                                    "$use": {
                                        "$literal": "Sim"
                                    }
                                },
                                {
                                    "$case": {
                                        "pertence_contador": false
                                    },
                                    "$use": {
                                        "$literal": "Não"
                                    }
                                }
                            ]
                        }
                    },
                    "Razão Social": "super$.razao_social",
                    "Score": "validacao_score",
                    "Status": "status_ligacao",
                    "Telefone Completo": "telefone_completo",
                    "WhatsApp": {
                        "$switch": {
                            "$cases": [
                                {
                                    "$case": {
                                        "whatsapp": true
                                    },
                                    "$use": {
                                        "$literal": "Sim"
                                    }
                                },
                                {
                                    "$case": {
                                        "whatsapp": false
                                    },
                                    "$use": {
                                        "$literal": "Não"
                                    }
                                }
                            ]
                        }
                    }
                },
                "$prop": "phones"
            }
        },
        "Veículos": {
            "$for": {
                "$format": {
                    "Ano de Fabricação": {
                        "$fallback": ["ano", "ano_fabricacao"]
                    },
                    "Aro": "aro",
                    "CNPJ": {
                        "$ncnpj": "super$.cnpj"
                    },
                    "Grupo": "grupo",
                    "Marca": "marca",
                    "Modelo": "modelo",
                    "Placa": "placa",
                    "Razão Social": "super$.razao_social",
                    "Tipo de Veículo": "tipo_veiculo"
                },
                "$prop": "veiculos"
            }
        }
    }
}
